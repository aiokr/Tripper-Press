<!-- Footer -->
<footer class="mdui-text-center itp-footer">
  <section class="footer-info">
    <%- config.title %> &copy; <%- theme.start_time %> - <%= (new Date()).getFullYear() %>
    <% if (theme.count == true){ %>
    <span> / </span>
    <%- symbolsCountTotal(site) %>
    <% } %>
    <% if (theme.icp){ %>
    <span> / </span>
    <a href="http://www.beian.miit.gov.cn"><%- theme.icp %></a>
    <% } %>
    <% if (theme.busuanzi == true){ %>
      <span> / </span>
      <span id="busuanzi_container_site_pv">PV <span id="busuanzi_value_site_pv"></span></span>
    <% } %>
  </section>
  <% if (theme.cdn.use == 'upyun'){ %>
  <section class="footer-logo">
    <a href="https://www.upyun.com/?utm_source=lianmeng&utm_medium=referral" target="_blank"><img class="cdn-img"
        src="<%= theme.cdn.upyun.src %>"></a>
    <% } %>
  </section>
</footer>