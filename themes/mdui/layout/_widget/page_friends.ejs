<%- cssLsload({path:(config.root + 'css/src/pages/' + page.layout + '.min'),key: page.layout + '_css'}) %>
<div class="mdui-container">
  <div class="mdui-row">
    <div class="mdui-col-xs-offset
                  mdui-col-sm-1
                  mdui-col-md-2
                  mdui-col-lg-2"></div>
    <div class="mdui-col-xs-12
                  mdui-col-sm-10
                  mdui-col-md-8
                  mdui-col-lg-8">
      <div class="padding"></div>
      <div class="feed-header">
        <div class="feed-title">
          合作伙伴列表
        </div>
        <div class="feed-subtitle">
          Partners List
        </div>
        <div class="padding"></div>
      </div>
      <% if (site.data.friends) { %>
        <% for (var i in site.data.friends) { %>
          <% if (site.data.friends[i].down !== true) { %>
            <div class="mdui-card">
              <div class="mdui-card-primary">
              <div class="mdui-card-primary-title">
                  <%= i %>
                </div>
                <div class="mdui-card-primary-subtitle">
                  <%= site.data.friends[i].subtitle %>
                </div>
              </div>
              <div class="mdui-card-content">
                <%= site.data.friends[i].descr %>
             </div>
              <div class="mdui-card-actions">
                <a class="mdui-btn mdui-ripple" href="<%= site.data.friends[i].link %>">
                  <%= __('friends.visit') %>
                </a>
              </div>
            </div>
            <div class="padding"></div>
          <% } %>
        <% } %>
        <%-partial('../_partials/fab/index')%>
      <% } %>
    </div>
    <div class="mdui-col-xs-offset
                  mdui-col-sm-1
                  mdui-col-md-2
                  mdui-col-lg-2"></div>
  </div>
</div>